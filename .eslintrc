{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "TweenMax": true,
    "TweenLite": true,
    "TimelineMax": true,
    "Sine": true,
    "Power1": true,
    "Power2": true,
    "Power3": true,
    "Power4": true,
    "Back": true,
    "Elastic": true,
    "Linear": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "max-len": 0,
    "arrow-body-style": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.test.babel.js"
      }
    }
  }
}
