{
    "name": "backfire",
    "version": "0.6.0",
    "description": "A boilerplate for React.js focused on animation using react-gsap-enhancer with redux, react-sagas, webpack, stylus, postcss plugins.",
    "scripts": {
        "clean": "rimraf build",
        "clean:modules": "rimraf node_modules",
        "build": "npm run clean && npm run build:webpack && open .",
        "build:webpack": "NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --progress --colors -p",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "NODE_ENV=development node webpack/server.js",
        "start": "npm run dev",
        "lint": "eslint src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/amorino/backfire.git"
    },
    "keywords": [
        "react",
        "post-css",
        "gsap",
        "animation",
        "stylus",
        "sagas",
        "webpack",
        "redux"
    ],
    "author": "Erick Baz√°n",
