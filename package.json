{
  "name": "backfire",
  "version": "0.6.0",
  "description": "A boilerplate for React.js focused on animation using react-gsap-enhancer with redux, react-sagas, webpack, stylus, postcss plugins.",
  "scripts": {
    "clean": "rimraf build",
    "clean:modules": "rimraf node_modules",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "webpack --config webpack/prod.config.babel.js",
    "dev": "NODE_ENV=development node -r babel-register webpack/server.js",
    "start": "npm run dev",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amorino/backfire.git"
  },
  "keywords": [
    "react",
    "gsap",
    "animation",
    "sagas",
    "webpack",
    "redux"
  ],
